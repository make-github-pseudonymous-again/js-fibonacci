{
  "name": "@integer-sequence/fibonacci",
  "description": "Fibonacci numbers for JavaScript",
  "version": "4.0.0",
  "author": "make-github-pseudonymous-again",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "timeout": "1m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/integer-sequence/fibonacci/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-array": "4.0.0",
    "@aureooms/js-number": "4.0.0",
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.14.2",
    "@babel/register": "7.13.16",
    "@iterable-iterator/range": "^1.0.0",
    "ava": "3.15.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "7.5.0",
    "nyc": "17.1.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://integer-sequence.github.io/fibonacci",
  "keywords": [
    "algorithms",
    "computer science",
    "fibonacci",
    "integer sequences",
    "javascript",
    "js",
    "number theory",
    "numbers"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/integer-sequence/fibonacci.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
